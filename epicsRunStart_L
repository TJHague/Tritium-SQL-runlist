log() { # debug echo - send it to /dev/null when not needed
    echo $* >> $epicsfile
#    echo $* >>  $SQLFILE
    if [ ! -z "$PS1" ] ; then echo $* ; fi
}
#
# make it easy to turn on/off (1/0) stuff (dont just comment it out!)
    TURNON0=0      # Target info  ####This seems to be out of use
    TURNON1=1      # General info
    TURNON1S=0     # Septum Magnets
    TURNON1L=1     # HRS Left Magnets 
    TURNON1R=0     # HRS Right Magnet
    TURNON2=0      # Bigbite magnets  ###NOT NEEDED
    TURNON3L=0     # HRS Left VDCs 
    TURNON3R=0     # HRS Right VDCs 
    TURNON4=0      # RICH Detector Stuff ###NOT NEEDED
    TURNON5=1     # BEAM info 
    TURNON5b=0    ### BCM gain info (sweems like same as 5)
    TURNON6L=1    # Left Arm Collimator and angles 
    TURNON6R=0     # Right Arm Collimator and Angles  
    TURNON7A=1     # injector/polarized beam info 
    TURNON7B=1     # Target OTR info
    TURNON7C=1     # ARC OTR info
    TURNON7C=0     # ARC OTR info
    TURNON7D=1     # Moeller quardupole
    TURNON8=1      # Cryotarget 
    TURNON9=0      # Gen thresholds for BigHnad and BigBite
    TURNON10=0     # 3he target variables  ###NOT NEEDED 
    TURNON11=0     # SLI data
    TURNON12=0     # Waterfall target
    TURNON13=0     # Left Arm FPP Stuff 
    TURNON14=0     # Calorimeter *******turn on for LEDEX!!Still need IP's.
    TURNON20=0     # CSR HV
    TURNON21LS2=0  # HRS Left S2 HV
    TURNON21LS1=0  # HRS Left S1 HV
    TURNON21LCER=0 # HRS Left Cerenkov HV
    TURNON21LA1=0  # HRS Left A1 Cerenkov HV
    TURNON21LPR1=0 # HRS Left Pion Rejector Layer 1 HV
    TURNON21LPR2=0 # HRS Left Pion Rejector Layer 2 HV
    TURNON21RS2=0  # HRS Right S2 HV
    TURNON21RS1=0  # HRS Right S1 HV
    TURNON21RS2m=0 # HRS Right S2m HV
    TURNON21RCER=0 # HRS Right Cerenkov HV
    TURNON21RPS=0  # HRS Right Preshower HV
    TURNON21RSH=0  # HRS Right Shower HV
    TURNON22=0     # Left Debugging
    TURNON23=0     # Right Debugging
    TURNON24L=0    # Left arm det/scint/S0 for PREX
    TURNON25=0 # Det encoder positions for PREX
    TURNON26=0 # PREX Septum 
    TURNON27=0 # DVCS calo
#   TURNON27=0 # DVCS calo
    TURNON28=0 # SRC (E07006) HV crates
    TURNON29=0 # Septum Right/Left current readback
    TURNON30=0 # Temporary Target Status
    TURNON31=0     # LHRS HV
    TURNON32=0     # RHRS HV
    TURNON33=1    # LHRS HV
    TURNON34=0     # Gas Flow
    TURNON35Lalign=0     # Left alignment
    TURNON35Ralign=0     # Right alignment
    TURNONMLU=0 #MLU info
    

